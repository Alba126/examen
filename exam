# !/usr/bin/env python3
# -*- coding: utf-8 -*-

import math

class Angle:

    n = int(input())
    M = []
    for i in range(n):
        s = input()
    M.append(s)
    print(M)

    print('Углы в радианах')
    R = []
    r = 0
    for i in range(n):
        k = float(M[i])
    r = ((k * 180) / 3.14)
    R.append(r)
    print(R)
